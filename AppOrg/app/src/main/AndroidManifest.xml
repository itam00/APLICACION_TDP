<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apporg">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_correcto"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_correcto"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Eventos.Editar_Evento_Activity" />
        <activity android:name=".Eventos.Agregar_Evento_Activity" />
        <activity
            android:name=".Eventos.Eventos_del_Dia"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".Eventos.Notification_receiver"></receiver>
    </application>

</manifest>